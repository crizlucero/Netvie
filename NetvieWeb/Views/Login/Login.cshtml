@model NetvieWeb.Models.Usuario
@{
    ViewBag.Title = "index";
}

<h2>index</h2>
@Html.BeginForm()
<table>
    <tbody>
        <tr>
            <td>Correo</td>
            <td>@Html.TextBoxFor( o => o.Correo)</td>
        </tr>
        <tr>
            <td>Password</td>
            <td>@Html.PasswordFor(o => o.Password)</td>
        </tr>
        <tr>
            <td colspan="2"><button type="submit" value="Iniciar"/></td>
        </tr>
        <tr>
            <td colspan="2">@Html.ActionLink("Olvide la contraseña", "")</td>
        </tr>
    </tbody>
</table>